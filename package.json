{
  "name": "ultrahtml",
  "type": "module",
  "version": "1.5.2",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/natemoo-re/ultrahtml"
  },
  "bugs": {
    "url": "https://github.com/natemoo-re/ultrahtml/issues"
  },
  "homepage": "https://github.com/natemoo-re/ultrahtml#README",
  "scripts": {
    "build": "node scripts/build.js",
    "lint": "prettier \"**/*.{js,ts,md}\"",
    "dev": "vitest",
    "test": "vitest run"
  },
  "files": [
    "selector.d.ts",
    "transform.d.ts",
    "jsx-runtime.d.ts",
    "transformers",
    "dist",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./selector": {
      "types": "./dist/selector.d.ts",
      "import": "./dist/selector.js"
    },
    "./transformers/*": {
      "types": "./dist/transformers/*.d.ts",
      "import": "./dist/transformers/*.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime/index.d.ts",
      "import": "./dist/jsx-runtime/index.js"
    }
  },
  "keywords": [
    "html",
    "template",
    "sanitize"
  ],
  "author": {
    "name": "Nate Moore",
    "email": "nate@natemoo.re",
    "url": "https://twitter.com/n_moore"
  },
  "license": "MIT",
  "volta": {
    "node": "18.7.0"
  },
  "packageManager": "pnpm@7.6.0",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/stylis": "^4.2.0",
    "chalk": "^5.3.0",
    "dts-bundle-generator": "^8.0.1",
    "esbuild": "^0.19.3",
    "globby": "^13.2.2",
    "gzip-size": "^7.0.0",
    "markdown-it": "^13.0.1",
    "media-query-fns": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "parsel-js": "^1.1.2",
    "prettier": "^3.0.3",
    "pretty-bytes": "^6.1.1",
    "stylis": "^4.3.0",
    "vitest": "^0.34.5"
  }
}
